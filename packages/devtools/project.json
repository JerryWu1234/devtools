{
  "name": "devtools",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/devtools/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/devtools",
        "configFile": "packages/devtools/vite.config.ts",
        "mode": "lib"
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["../..//coverage/packages/devtools"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/packages/devtools"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/devtools/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
